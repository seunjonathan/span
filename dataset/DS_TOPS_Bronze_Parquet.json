{
	"name": "DS_TOPS_Bronze_Parquet",
	"properties": {
		"description": "This data set is to store data in Delta lake",
		"linkedServiceName": {
			"referenceName": "LS_ADLS_SMG",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"pFileName": {
				"type": "string"
			},
			"pDirectory": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().pFileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('bronze/TOPSv2/',dataset().pDirectory)",
					"type": "Expression"
				},
				"fileSystem": "landing"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}