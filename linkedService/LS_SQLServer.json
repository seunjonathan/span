{
	"name": "LS_SQLServer",
	"properties": {
		"description": "This is SQL Server for TOPS on-prem data",
		"parameters": {
			"SQLUsername": {
				"type": "string",
				"defaultValue": "PowerBIReader_SMCBITeam"
			},
			"SQLServer": {
				"type": "string",
				"defaultValue": "van-ss-sql15ag.seaspan.com"
			},
			"SQLDatabase": {
				"type": "string",
				"defaultValue": "TOPSSEASPAN_PROD"
			},
			"SQLPassword": {
				"type": "string",
				"defaultValue": "TOPS-Password"
			}
		},
		"annotations": [],
		"type": "SqlServer",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Data Source=@{linkedService().SQLServer};Initial Catalog=@{linkedService().SQLDatabase};User ID=@{linkedService().SQLUsername}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "LS_KV_SMG",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().SQLPassword",
					"type": "Expression"
				}
			}
		},
		"connectVia": {
			"referenceName": "IR-SQLServer",
			"type": "IntegrationRuntimeReference"
		}
	}
}