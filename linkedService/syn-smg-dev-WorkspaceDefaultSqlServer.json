{
	"name": "syn-smg-dev-WorkspaceDefaultSqlServer",
	"type": "Microsoft.Synapse/workspaces/linkedservices",
	"properties": {
		"typeProperties": {
			"connectionString": "Data Source=tcp:syn-smg-dev.sql.azuresynapse.net,1433;Initial Catalog=@{linkedService().DBName}"
		},
		"parameters": {
			"DBName": {
				"type": "String"
			}
		},
		"type": "AzureSqlDW",
		"connectVia": {
			"referenceName": "AutoResolveIntegrationRuntime",
			"type": "IntegrationRuntimeReference"
		},
		"annotations": []
	}
}